div(id='nav')
    div(id='navContainer')
        h4(id='navTitle') Goto
        ul(id='navUl')
            li
                a(class='navA' href='/dash') Dash
            li
                a(class='navA'  href='/dash/map') Map
            if user.admin  === true
                li
                    a(class='navA'  href='/dash/admin') Admin
                li
                    a(class='navA'  href='/dash/load') Load
                li
                    a(class='navA' href ='/dash/logs') Logs
                li 
                    a(class='navA' href ='/dash/report') Report
            if user.admin === false
                li
                    a(class='navA onOverride' href='/dash/load' style='display:none;') Load
            li 
                a(class='navA'  href='/logout') Logout
    button(id='navToggler')
script.
        if(navConVisible){
            $('#navToggler').text('<')
            $('#navContainer').show()
        }
        else{
            $('#navContainer').hide()
            $('#navToggler').text('>')
        }
        runAnime = function () {
            navConVisible = !navConVisible
            $('#navContainer').animate({width: 'toggle'},
            200,
            function(){
                if(navConVisible){
                    $('#navToggler').text('<')
                }
                else{
                    $('#navToggler').text('>')
                }
            })
        }
        $('#navToggler').on('mouseenter', runAnime)
        $('#content').on('mouseenter',function(){
            if(navConVisible){
                runAnime()
            }
        })